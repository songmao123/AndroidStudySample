<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="PasswordEditView">
        <attr name="normalBorderColor" format="color" />
        <attr name="focusedBorderColor" format="color" />
        <attr name="normalPassDotColor" format="color" />
        <attr name="focusedPassDotColor" format="color" />
        <attr name="borderRadius" format="dimension" />
        <attr name="maxPassNum" format="integer" />
        <attr name="passDotRadius" format="dimension" />
        <attr name="focusAnim" format="boolean" />
